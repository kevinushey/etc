[core]
	excludesfile = ~/.gitignore
	editor = vim
	whitespace = -trailing-space
	pager = less
[push]
	autoSetupRemote = true
	default = current
[user]
	name = Kevin Ushey
	email = kevinushey@gmail.com
[init]
	defaultBranch = main
[diff]
	noprefix = true
[pull]
	rebase = true
[submodule]
	recurse = true
[credential]
	helper = cache --timeout=36000
[rerere]
	enabled = true
[url "git@github.com:"]
	insteadOf = https://github.com/
